    <Member MemberName="CustomDrop">
      <MemberSignature Language="C#" Value="public void CustomDrop (float Drop);" />
      <MemberSignature Language="ILAsm" Value=".method public hidebysig newslot virtual instance void CustomDrop([in]float32 Drop) runtime managed" />
      <MemberSignature Language="DocId" Value="M:Microsoft.Office.Interop.Word.CalloutFormat.CustomDrop(System.Single)" />
